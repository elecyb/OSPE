  }
  template <class T1, class T2>
  inline void __construct (T1* p, const T2& value)
  {
    new (p) T1(value);
  }

  template <class ForwardIterator> 
  _RWSTD_TRICKY_INLINE void __destroy (ForwardIterator first, ForwardIterator last)
  {
    while (first != last)
      ++first;
  }
